{
  "name": "cypress-automation",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run",
    "headTest": "npx cypress run --headed",
    "chromeTest": "npx cypress run --browser chrome",
    "recordDashBoardTest": "npx cypress run --record --key 5e71110c-ea4e-46c2-a810-8e975678c656",
    "ecommerceTest": "npx cypress run --spec \"cypress/integration/ecommerce/*\""
